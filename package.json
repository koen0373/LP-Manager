{
  "name": "liquilab-lp-manager",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "dev:clean": "rm -rf .next && next dev -p 3000 -H 0.0.0.0",
    "prebuild": "npm run verify:pricing",
    "build": "next build",
    "build:worker": "prisma migrate deploy && prisma generate && tsc --project tsconfig.worker.json && tsc-alias -p tsconfig.worker.json",
    "build:worker:local": "prisma generate && tsc --project tsconfig.worker.json && tsc-alias -p tsconfig.worker.json",
    "start": "next start -p $PORT",
    "lint": "eslint",
    "lint:ci": "eslint --max-warnings=0 src/lib/enrich src/lib/icons src/services/tokenIconService.ts src/features/poolDetail src/components/PoolDetailHeader.tsx src/components/WalletConnect.tsx src/components/waitlist src/components/demo/DemoPoolsTable.tsx src/components/pools/PoolRangeIndicator.tsx src/components/Header.tsx scripts/verify-enrichment",
    "postinstall": "prisma generate",
    "backfill:run": "node ./dist/scripts/backfillLedger.js",
    "backfill:ids": "node ./dist/scripts/backfillLedger.js 22003 22326 20445 21866",
    "backfill:wallet": "node ./dist/scripts/backfillLedger.js",
    "backfill": "tsx scripts/backfillLedger.ts",
    "backfill:railway": "sh scripts/railway-backfill.sh",
    "backfill:swaps": "tsx scripts/backfillPoolSwaps.ts",
    "indexer:backfill": "tsx scripts/indexer-backfill.ts",
    "indexer:backfill:ankr": "node -r tsconfig-paths/register scripts/indexer/backfill-ankr.mjs",
    "indexer:backfill:railway": "tsx scripts/indexer-backfill.ts --stream=nfpm --rps=2 --concurrency=2 --blockWindow=25",
    "indexer:follow": "tsx scripts/indexer-follower.ts",
    "indexer:follow:railway": "tsx scripts/indexer-follower.ts --stream=nfpm",
    "indexer:staking": "tsx scripts/indexer-staking.mts",
    "indexer:incentives": "tsx scripts/compute-incentives.mts",
    "indexer:incentives:api": "tsx scripts/fetch-api-incentives.mts",
    "indexer:sanity": "tsx scripts/indexer-sanity.ts",
    "railway:start": "sh scripts/railway-entrypoint.sh",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "devup": "bash scripts/dev_up.sh",
    "devdown": "bash scripts/dev_down.sh",
    "smoke": "bash liquilab_smoke.sh",
    "cache:top-pools": "tsx scripts/cache-top-pools.ts",
    "cache:top-pools:now": "tsx scripts/cache-top-pools.ts",
    "snapshot:brand": "pnpm run -s snapshot:brand:run",
    "snapshot:brand:run": "pnpm dlx tsx --env-file=.env.local scripts/data/resolve-brand-pools.ts && pnpm dlx tsx --env-file=.env.local scripts/data/snapshot-brand-user-positions.ts",
    "state:rotate": "node scripts/dev/rotate-project-state.mjs",
    "scan:nfpm": "DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config -r dotenv-expand/config scripts/dev/ankr-nfpm-scan.mts",
    "sql:backfill:tokenid-pool": "psql \\\"$RAW_DB\\\" -v ON_ERROR_STOP=1 -f scripts/dev/backfill-tokenid-pool.sql",
    "sql:refresh:analytics-flat": "psql \\\"$RAW_DB\\\" -v ON_ERROR_STOP=1 -f scripts/dev/refresh-analytics-flat.sql",
    "sql:verify:tokenid-pool": "psql \\\"$RAW_DB\\\" -v ON_ERROR_STOP=1 -f scripts/dev/verify-tokenid-pool.sql",
    "test": "vitest",
    "docs": "pnpm dlx docsify-cli@latest serve docs -p 3030",
    "docs:ui": "open http://localhost:3000/docs",
    "report:weekly": "node scripts/reports/weekly-liquidity-pool-report.mjs --week auto",
    "report:social": "node scripts/generate-social-media.js",
    "report:all": "bash scripts/weekly-report-workflow.sh",
    "verify:web": "node -r tsconfig-paths/register scripts/verify-web/port-and-health.mjs",
    "verify:indexer": "node scripts/verify-indexer/scan.mjs",
    "verify:indexer:plan": "node -r tsconfig-paths/register scripts/verify-indexer/backfill-plan.mjs",
    "verify:enrichment": "node scripts/verify-enrichment/registry.mjs",
    "verify:icons": "node scripts/verify-enrichment/icons.mjs",
    "verify:icons:local": "node scripts/verify-icons/no-remote-icons.mjs",
    "verify:mv": "node scripts/verify-enrichment/mv-health.mjs",
    "verify:report": "node scripts/verify-report/weekly.mjs",
    "verify:pricing": "node scripts/verify-pricing/drift.mjs",
    "verify:api:prices": "node scripts/verify-api/prices-current.mjs",
    "scan:prices": "node scripts/scan/prices-sources.mjs",
    "verify": "npm run -s lint:ci && npm run -s scan:prices && npm run -s verify:api:prices && npm run -s verify:icons",
    "icons:collect": "node scripts/icons/collect-flare-dex-tokens.mjs",
    "icons:fetch": "node scripts/icons/fetch-dex-icons.mjs",
    "icons:probe": "node scripts/verify-icons/remote-probe.mjs",
    "icons:test:topic": "node scripts/verify-icons/topic-matches.mjs --rpc=$FLARE_RPC_PRIMARY --factory=$FACT --from=$FROM --to=$TO"
  },
  "dependencies": {
    "@ankr.com/ankr.js": "^0.6.1",
    "@perplexity-ai/perplexity_ai": "^0.16.0",
    "@prisma/client": "6.19.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "5.90.5",
    "@wagmi/connectors": "^3.1.5",
    "@wagmi/core": "^2.22.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "dayjs": "^1.11.18",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "ethers": "^5.7.2",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "p-limit": "^6.2.0",
    "pg": "^8.16.3",
    "qrcode-generator": "^2.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.3.0-canary.4",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tsconfig-paths": "^4.2.0",
    "viem": "2.38.5",
    "wagmi": "2.19.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/qrcode-generator": "^0.0.16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^4.0.7",
    "autoprefixer": "^10.4.16",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "postcss": "^8.4.32",
    "prisma": "6.19.0",
    "tailwindcss": "^3.4.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^4.0.7"
  }
}